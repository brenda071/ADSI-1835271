doctype
html(lang="es")
	head
		meta(charset="UTF-8")
		title Código
	body
		h1 Código
		- var colores = ['purple','aqua','teal','lime']
		ul 
			each color in colores
				li(style={color: color})=color
				li(style={background: color})=color
			br
			table(border=5)
				- for(var i = 1; i <= 10; i++)
					tr 
						-for(var j = 1; j<= 20; j++)
							td f#{i}  c#{j}

